{
  "name": "rodeo",
  "productName": "Rodeo",
  "version": "2.5.1",
  "description": "Rodeo: An IDE for data science",
  "author": "Yhat Inc. <team@yhathq.com> (https://www.yhat.com/products/rodeo)",
  "license": "AGPL-3.0",
  "main": "app/node",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/yhat/rodeo.git"
  },
  "scripts": {
    "start": "electron app",
    "hot-server": "node -r babel-register server.js",
    "test": "./node_modules/.bin/jest src/browser --coverage",
    "build": "npm run build:assets && npm run build:browser && npm run build:node",
    "build:assets": "NODE_ENV=production ./node_modules/.bin/gulp",
    "build:browser": "NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.js --progress --profile --colors",
    "build:browser-dev": "NODE_ENV=development node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.dev.config.js --progress --profile --colors",
    "build:node": "NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.main.config.js --progress --profile --colors",
    "dist:osx": "./scripts/osx/dist-osx.sh",
    "dist:win": "./scripts/osx/dist-win.sh",
    "dist:linux": "./scripts/osx/dist-linux.sh"
  },
  "build": {
    "appId": "com.yhat.rodeo",
    "asar": true,
    "publish": [
      {
        "provider": "generic",
        "url": "http://rodeo-nsis.yhat.com/asdf",
        "channel": "latest"
      }
    ],
    "win": {
      "target": [
        "squirrel"
      ],
      "extraResources": [
        {
          "from": ".tmp/win-64/conda/",
          "to": "conda",
          "filter": "**/*"
        }
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "tar.gz",
        "zip"
      ]
    },
    "mac": {
      "helperBundleId": "com.yhat.rodeo.helper",
      "target": [
        "dmg",
        "zip"
      ]
    }
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-jest": "^13.2.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-lodash": "^3.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babelify": "^7.3.0",
    "bootstrap": "^3.3.7",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.23.1",
    "electron": "^1.4.10",
    "electron-builder": "7.24.1",
    "enzyme": "^2.6.0",
    "eslint": "^2.7.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.0.1",
    "file-loader": "^0.8.5",
    "font-awesome": "^4.6.1",
    "globby": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-less": "^3.0.5",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "jest": "^15.1.1",
    "jquery": "^2.2.3",
    "json-loader": "^0.5.4",
    "karma": "^1.3.0",
    "less": "^2.5.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.17.2",
    "minimist": "^1.1.1",
    "raw-loader": "^0.5.1",
    "react": "^15.2",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-test-utils": "^15.2.1",
    "react-dom": "^15.2",
    "react-hot-loader": "^3.0.0-beta.1",
    "react-redux": "^4.4",
    "react-test-renderer": "^15.3.1",
    "redux": "^3.5",
    "redux-actions": "^0.13.0",
    "redux-devtools": "^3.2.0",
    "redux-thunk": "^2.0.1",
    "reselect": "^2.5.4",
    "rimraf": "^2.4.3",
    "rulejs": "^0.1.0",
    "sandboxed-module": "^2.0.2",
    "strip-ansi": "^3.0.1",
    "style-loader": "^0.13.1",
    "svgo": "^0.6.6",
    "svgo-loader": "^1.1.0",
    "uglify-js": "^2.4.24",
    "url-loader": "^0.5.7",
    "vinyl-map": "^1.0",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-node-externals": "^1.5.4",
    "webpack-stream": "^3.2.0",
    "yaml-loader": "^0.1.0"
  },
  "dependencies": {
    "ansi-to-html": "^0.4.1",
    "babel-register": "^6.8.0",
    "bluebird": "^3.3.4",
    "body-parser": "^1.15.0",
    "bootstrap": "^3.3.6",
    "chalk": "^1.1.3",
    "chokidar": "^1.6.0",
    "colors": "^1.1.2",
    "copy": "^0.3.0",
    "cross-env": "^3.1.3",
    "cuid": "^1.3.8",
    "eventify": "^2.0.0",
    "express": "^4.13.4",
    "find-port": "^2.0.1",
    "fixed-data-table-2": "^0.7.7",
    "highlight.js": "^8.9.1",
    "marked": "^0.3.5",
    "node-fetch": "^1.5.0",
    "pg": "^6.1.0",
    "seamless-immutable": "^6.1.1",
    "source-map-support": "^0.4.6",
    "stream-splitter": "^0.3.2",
    "temp": "^0.8.3",
    "uuid": "^2.0.2",
    "winston": "^2.2.0",
    "xml2js": "^0.4.17",
    "yargs": "^4.6.0"
  },
  "jest": {
    "bail": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/browser/jsx/**/*{.js,.jsx}"
    ],
    "moduleNameMapper": {
      "^ace$": "<rootDir>/__mocks__/ace.js",
      "(raw!.*|.*css|.*yml|.*md|.*py)": "<rootDir>/__mocks__/generic-stub.js"
    },
    "setupFiles": [
      "<rootDir>/__mocks__/browser-mocks.js"
    ],
    "testRegex": "/src/browser/.*\\.test\\.js$"
  }
}
